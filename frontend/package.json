{
  "name": "lightslark-server",
  "version": "1.0.0",
  "license": "LGPL-3.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --target=development --sourcemaps --progress --poll=2000",
    "build": "ng build --preserve-symlink --target=development --outputPath=../gen/web/assets/web --sourcemaps --progress",
    "prod": "ng build --preserve-symlink --target=production --aot --outputPath=../gen/web/assets/web --vendorChunk --baseHref \"/\" --verbose --progress",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.7",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.5.3"
  }
}
